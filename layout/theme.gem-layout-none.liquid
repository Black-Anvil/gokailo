  <script src="https://cdn-widgetsrepository.yotpo.com/v1/loader/6VubeMJ7GY2e0Ai0ynXuSw" async></script>
</head>

<body>
<div class="gem-container">
	{{ content_for_layout }}
</div><!-- end .gem-container -->
